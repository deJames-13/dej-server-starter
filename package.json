{
  "name": "@dejames-13/dej-server-starter",
  "version": "0.0.1",
  "description": "A starter template for Express Server projects, for personal use only",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "server": "nodemon server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/deJames-13/dej-server-starter.git"
  },
  "imports": {
    "#controllers": "./controllers/index.js",
    "#middlewares": "./middlewares/index.js",
    "#validations": "./validations/index.js",
    "#models": "./models/index.js",
    "#resources": "./resources/index.js",
    "#services": "./services/index.js",
    "#utils": "./utils/index.js",
    "#controllers/*": "./controllers/*.js",
    "#middlewares/*": "./middlewares/*.js",
    "#validations/*": "./validations/*.js",
    "#models/*": "./models/*.js",
    "#resources/*": "./resources/*.js",
    "#services/*": "./services/*.js",
    "#utils/*": "./utils/*.js"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-async-handler": "^1.2.0",
    "express-validator": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.5.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "eslint": "^9.9.0",
    "globals": "^15.9.0",
    "nodemon": "^3.1.4"
  },
  "keywords": [
    "express",
    "starter",
    "server",
    "template"
  ],
  "author": "deJames-13 <de.james013@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/deJames-13/dej-server-starter/issues"
  },
  "homepage": "https://github.com/deJames-13/dej-server-starter#readme"
}
